include Makefile
PREFIX = @prefix@
CPPFLAGS += -include @abs_top_srcdir@/config.h -I @abs_top_srcdir@/lib -D_GNU_SOURCE=
CFLAGS += -Wno-int-to-pointer-cast -Wno-pointer-to-int-cast
SHAREDLIB_EXT = dll
SHAREDLIB_LINK_OPTIONS = -shared
LIBFDT_lib = $(LIBFDT_objdir)/libfdt.$(SHAREDLIB_EXT)
LIBFDT_soname =
$(BIN): @abs_top_srcdir@/lib/libgnu.a

$(LIBFDT_lib):
	@$(VECHO) LD $@
	$(CC) $(LDFLAGS) $(SHAREDLIB_LINK_OPTIONS) -o $(LIBFDT_lib) $^

install-lib: all
	@$(VECHO) INSTALL-LIB
	$(INSTALL) -d $(DESTDIR)$(LIBDIR)
	$(INSTALL) $(LIBFDT_lib) $(DESTDIR)$(LIBDIR)
	$(INSTALL) -m 644 $(LIBFDT_archive) $(DESTDIR)$(LIBDIR)
